{
  "version": "0.2.0",
  "configurations": [
      {
          "name": "Python: entrySystem",
          "type": "debugpy",
          "request": "launch",
          "program": "${workspaceFolder}/entrySystem.py",
          "console": "integratedTerminal",
          "envFile": "${workspaceFolder}/.env",
          "justMyCode": false,
          "subProcess": true,
          "args": ["-Xfrozen_modules=off"],
          "env": {
              "PYDEVD_DISABLE_FILE_VALIDATION": "1"
          }
      }
  ]
}